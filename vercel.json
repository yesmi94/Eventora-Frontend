{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/api/*",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowedRoles": ["anonymous"],
      "proxy": "https://web-app-event-management-backend.azurewebsites.net/api/{path}",
      "headers": {
        "Authorization": "{request.headers['authorization']}"
      }
    }
  ]
}